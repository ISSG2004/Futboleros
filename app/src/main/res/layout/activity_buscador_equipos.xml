<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BuscadorEquiposActivity">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recycler_equipos"
        android:layout_width="401dp"
        android:layout_height="397dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5" />

    <SearchView
        android:id="@+id/search_view"
        android:layout_width="397dp"
        android:layout_height="51dp"
        android:layout_marginBottom="88dp"
        app:layout_constraintBottom_toTopOf="@+id/recycler_equipos"
        app:layout_constraintEnd_toEndOf="@+id/recycler_equipos"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/recycler_equipos" />
</androidx.constraintlayout.widget.ConstraintLayout>